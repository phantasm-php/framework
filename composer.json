{
    "name": "punyflash/php-framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Morozov",
            "email": "puny.flash@gmail.com"
        }
    ],
    "require": {
        "psr/container": "^2.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "pestphp/pest": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "WeStacks\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "framework": {
            "discover": true
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable"
}
