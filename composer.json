{
    "name": "phantasm/framework",
    "type": "metapackage",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Morozov",
            "email": "puny.flash@gmail.com"
        }
    ],
    "repositories": {
        "local": {
            "type": "path",
            "url": "./packages/*"
        }
    },
    "require": {
        "php": "^8.4",
        "phantasm/console": "dev-main",
        "phantasm/container": "dev-main",
        "phantasm/contracts": "dev-main",
        "phantasm/foundation": "dev-main"
    },
    "require-dev": {
        "carthage-software/mago": "^0.26.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "carthage-software/mago": true
        }
    }
}
